{
  "private": true,
  "name": "valibot-benchmarks",
  "type": "module",
  "scripts": {
    "build": "./node_modules/.bin/tsx ./npm-scripts/build.ts",
    "ci:pickup-orders": "npm run clean && ./node_modules/.bin/tsx ./npm-scripts/ci-pickup-orders.ts",
    "ci:finalize-pending-tasks": "npm run clean && ./node_modules/.bin/tsx ./npm-scripts/finalize-pending-tasks.ts",
    "bench": "npm run build && ./node_modules/.bin/tsx src/runner.ts",
    "bench:nodejs": "mkdir -p results && NODE_ENV=production node ./src/runners/nodejs.js results/nodejs.json",
    "bench:deno": "mkdir -p results && deno run --no-npm --allow-write=./results/deno.json ./src/runners/nodejs.js ./results/deno.json",
    "bench:bun": "mkdir -p results && bun ./src/runners/nodejs.js ./results/bun.json",
    "clean": "npm run clean:libs && rm -rf dist && mkdir dist",
    "clean:libs": "rm -rf ./libs/*",
    "experimental:profile": "NODE_ENV=production ./node_modules/.bin/0x --output-dir=0x ./src/runners/nodejs.js",
    "experimental:debugger": "NODE_ENV=production node --inspect-brk ./src/runners/nodejs.js",
    "experimental:hono": "NODE_ENV=production ./node_modules/.bin/tsx ./src/hono.ts",
    "experimental:browser": "./node_modules/.bin/tsx ./src/runners/browser.ts 3000",
    "next:dev": "./node_modules/.bin/next",
    "next:build": "./node_modules/.bin/next build",
    "lint": "npm run lint:format",
    "eslint": "./node_modules/.bin/eslint --report-unused-disable-directives --max-warnings 0 .",
    "eslint:interactive": "./node_modules/.bin/eslint-interactive --report-unused-disable-directives --max-warnings 0 .",
    "lint:knip": "./node_modules/.bin/knip",
    "lint:type": "./node_modules/.bin/tsc",
    "lint:format": "./node_modules/.bin/prettier --check .",
    "lint:format:fix": "./node_modules/.bin/prettier --write ."
  },
  "dependencies": {
    "@babel/preset-typescript": "7.22.5",
    "@eslint/js": "8.47.0",
    "@hono/node-server": "1.1.1",
    "@types/d3-array": "3.0.5",
    "@types/node": "20.5.0",
    "@visx/xychart": "3.3.0",
    "0x": "5.6.0",
    "babel-loader": "9.1.3",
    "d3-array": "3.2.4",
    "hono": "3.4.1",
    "next": "13.4.13",
    "playwright": "1.37.0",
    "quickjs-emscripten": "0.23.0",
    "swr": "2.2.1",
    "terser": "5.19.2",
    "ts-pattern": "5.0.5",
    "tsx": "3.12.7",
    "typescript": "5.1.6",
    "valibot": "0.12.0",
    "vitest": "0.34.1",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4",
    "zod": "3.21.4",
    "zx": "7.2.3"
  },
  "devDependencies": {
    "@types/react": "18.2.20",
    "@typescript-eslint/eslint-plugin": "6.3.0",
    "@typescript-eslint/parser": "6.3.0",
    "autoprefixer": "10.4.15",
    "eslint": "8.47.0",
    "eslint-interactive": "10.8.0",
    "knip": "2.19.3",
    "postcss": "8.4.27",
    "prettier": "3.0.1",
    "tailwindcss": "3.3.3"
  }
}
